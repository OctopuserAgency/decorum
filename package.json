{
  "name": "decorum",
  "version": "0.2.0-alpha",
  "description": "An ES7 decorator-based validation framework",
  "main": "dist/decorum.js",
  "scripts": {
    "build": "node webpack.js",
    "preperformance": "tsc --outDir tests-compile",
    "performance": "node --inspect --inspect-brk tests-compile/performance-test.js",
    "postperformance": "rm -rf ./tests-compile",
    "pretest": "tsc --outDir tests-compile",
    "test": "istanbul cover -e .ts -x \"*.d.ts\" ./node_modules/mocha/bin/mocha tests-compile/tests/***/*.js -- -R spec --recursive",
    "posttest": "rm -rf ./tests-compile",
    "test-karma": "karma start karma.conf.js",
    "pretestinspect": "tsc --outDir tests-compile",
    "testinspect": "istanbul cover -e .ts -x \"*.d.ts\" ./node_modules/mocha/bin/mocha tests-compile/tests/index.js -- -R spec --recursive --inspect --inspect-brk",
    "posttestinspect": "rm -rf ./tests-compile",
    "prepack": "node webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dflor003/decorum.git"
  },
  "keywords": [
    "validation",
    "validations",
    "validator",
    "decorator",
    "decorators",
    "es7",
    "es2016",
    "typescript",
    "ts",
    "decorum",
    "library",
    "babel"
  ],
  "author": "DanilF <dflor003@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dflor003/decorum/issues"
  },
  "homepage": "https://github.com/dflor003/decorum#readme",
  "devDependencies": {
    "bluebird": "^2.10.2",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "@types/sinon": "^2.3.6",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "dreamjs": "^0.1.8",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-typescript": "^1.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-jsdom-launcher": "^6.1.2",
    "jsdom": "^11.3.0",
    "karma-sinon": "^1.0.5",
    "karma-typescript": "^3.0.8",
    "karma-webpack": "^2.0.5",
    "mocha": "^3.4.2",
    "ora": "^1.3.0",
    "phantomjs-prebuilt": "^2.1.15",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "typescript-eslint-parser": "^8.0.0",
    "webpack": "^3.5.4",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {}
}
